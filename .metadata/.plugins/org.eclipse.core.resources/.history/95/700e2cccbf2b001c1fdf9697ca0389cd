package LabTwo;

public class Lotto {
	public int[] lottoNumbers;
		
	public Lotto() {		
		int max = 9;
		int min = 1;
		int range = max-min + 1; // Establish range for random number generator
		
		for (int i = 0; i < 10; i++) {
            lottoNumbers[0] = (int)(Math.random() * range) + min;} // generate the first number
        for (int i = 0; i < 10; i++) {
            lottoNumbers[1] = (int)(Math.random() * range) + min;} // generate the second number
        for (int i = 0; i < 10; i++) {
            lottoNumbers[2] = (int)(Math.random() * range) + min;} // generate the third number         
	}
	
	public void DisplayArray() {
		System.out.println("Element at index 0: " + lottoNumbers[0]);
		System.out.println("Element at index 1: " + lottoNumbers[1]);
		System.out.println("Element at index 2: " + lottoNumbers[2]);
	}
	
	
}

